	<RadzenStack Orientation="Orientation.Vertical">

		<RadzenCard class="rz-my-12 rz-mx-auto">
			<RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Start" Gap="1rem" Wrap="FlexWrap.Wrap">
				<label for="resetbutton" id="resetbuttonlabel" class="reset-button-label noselect">
					<input id="resetbutton" type="button" />
					Reset
				</label>
			</RadzenStack>

			<RadzenStack>
				<div id="collapsebox" class="collapse-box" style="max-height: 500px;">
					<div class="h-separator"></div>
					<div class="advanced-settings-box">
						<form action id="strat" class="strat-form">
							<h4>Strategy</h4>
							<div class="settings-container">
								<div>
									<input type="radio" id="strat1" name="strat" value="strat1" checked>
									<label for="strat1">Probability</label>
								</div>
								<div>
									<input type="radio" id="strat2" name="strat" value="strat2">
									<label for="strat2">Swords</label>
								</div>
								<div>
									<input type="radio" id="strat3" name="strat" value="strat3">
									<label for="strat3">Coffer</label>
								</div>
								<div>
									<input type="radio" id="strat4" name="strat" value="strat4">
									<label for="strat4">Fox</label>
								</div>
							</div>
							<div class="settings-container">
								<div>
									<input type="radio" id="strat5" name="strat" value="strat5">
									<label for="strat5">Weighted</label>
								</div>
								<div class="psuedo"></div>
								<div>
									<input type="checkbox" id="spreadsheet">
									<label for="spreadsheet">Fox Sightings</label>
								</div>
								<div>
									<input type="checkbox" id="lookforfox" checked>
									<label for="lookforfox">Look for Fox</label>
								</div>
							</div>
							<div class="settings-container">

								<div style="width: 140px;">
									Weights
									<div>
										<input type="number" id="foxweight" value="100" pattern="[0-9]" min="0" max="100" class="weights">
										<label>Fox</label>
									</div>
									<div>
										<input type="number" id="boxweight" value="35" pattern="[0-9]" min="0" max="100" class="weights">
										<label id="boxweightlabel">Coffer</label>
									</div>
									<div>
										<input type="number" id="swordweight" value="30" pattern="[0-9]" min="0" max="100" class="weights">
										<label>Swords</label>
									</div>
								</div>
							</div>
						</form>
						<div class="settings-container settings-right-align">
							<form action class="box-type">
								<div>
									<label for="box1">Coffer</label>
									<input type="radio" id="box1" name="boxtype" value="box1" onclick="UpdateCoffer();" checked>
								</div>
								<div>
									<label for="box2">Gift Box</label>
									<input type="radio" id="box2" name="boxtype" value="box2" onclick="UpdateCoffer();">
								</div>
							</form>
							<div class="psuedo"></div>
							<div>
								<label for="showstats">Show Stats</label>
								<input type="checkbox" id="showstats" onclick="ScoresCheckUpdate();">
							</div>
							<div>
								<label for="liveupdate">Live Update</label>
								<input type="checkbox" id="liveupdate" onclick="LiveUpdate(); ScoresCheckUpdate();" checked>
							</div>
						</div>
					</div>
				</div>
			</RadzenStack>
		</RadzenCard>
	</RadzenStack>

